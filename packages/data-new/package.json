{
  "name": "@gi-tcg/data",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build:entry": "node ./scripts/generate_imports.js",
    "build:rollup": "rollup -c",
    "build:types": "dts-bundle-generator ./src/index.ts -o ./dist/index.d.ts --export-referenced-types=false --no-check",
    "build": "pnpm build:entry && pnpm build:rollup && pnpm build:types"
  },
  "dependencies": {
    "@gi-tcg/typings": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "dts-bundle-generator": "^8.0.1",
    "glob": "^10.2.7",
    "rollup": "^3.25.0",
    "tsx": "^3.12.7"
  }
}
